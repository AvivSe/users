<div id="messageArea" class="row">

    <div class="col-md-4">
        <div class="wall">
            <h3>Online Users</h3>
            <ul class="list-group" id="users"></ul>
        </div>
    </div>

    <div class="col-md-8">
        <div class="chat" id="chat">
            <form id="messageForm">
                <div class="form-group">
                    <label>Enter Msg</label>
                    <input class="form-control" id="message">
                    <br />
                    <input type="submit" class="btn btn-primary" value="Send msg" />

                </div>
            </form>
        </div>
    </div>

</div>